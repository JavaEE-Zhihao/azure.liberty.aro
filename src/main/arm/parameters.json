{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/src/main/"
        },
        "identity": {
            "value": {
                "type": "userAssigned",
                "userAssignedIdentities": {
                    "${identityId}": {}
                }
            }
        },
        "createCluster": {
            "value": "${createCluster}"
        },
        "clusterName": {
            "value": "${clusterName}"
        }, 
        "clusterRGName": {
            "value": "${clusterRGName}"
        },
        "deployApplication": {
            "value": "${deployApplication}"
        },
        "appImagePath": {
            "value": "${appImagePath}"
        },
        "appReplicas": {
            "value": "${appReplicas}"
        }
    }
}